    </membership>
    <profile automaticSaveEnabled="true">
      <properties>
        <add name="UserLevel" type="String" defaultValue="2" readOnly="false"/>
        <add name="OpCntrs" type="System.Collections.Specialized.StringCollection" readOnly="false" serializeAs="Xml"/>
        <add name="Regions" type="System.Collections.Specialized.StringCollection" readOnly="false" serializeAs="Xml"/>
        <add name="Level" type="String" defaultValue="5" readOnly="false"/>
        <add name="FullName" type="String" defaultValue="" readOnly="false"/>
        <add name="EmployeeNumber" type="String" defaultValue="" readOnly="false"/>
        <add name="Address" type="String" defaultValue="" readOnly="false"/>
        <add name="City" type="String" defaultValue="" readOnly="false"/>
        <add name="State" type="String" defaultValue="" readOnly="false"/>
        <add name="Zip" type="String" defaultValue="" readOnly="false"/>
        <add name="Phone" type="String" defaultValue="" readOnly="false"/>
        <add name="Mobile" type="String" defaultValue="" readOnly="false"/>
        <add name="Fax" type="String" defaultValue="" readOnly="false"/>
        <add name="LastDivision" type="Int32" defaultValue="0" readOnly="false"/>
      </properties>
    </profile>
