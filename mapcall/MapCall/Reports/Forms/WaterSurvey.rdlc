      <Query>
        <DataSourceName>MCProd</DataSourceName>
        <CommandText>SELECT
	a.[RecID],
	a.[Name],
	IsNull(CAST(a.[MailStNum] AS varchar) + ' ', '') + a.[MailStName] AS [MailAddress],
	a.[MailTown] AS [MailCity],
	a.[MailState],
	a.[MailZip],
	a.[ServNum] AS [ServiceNumber],
	a.[PurpInstal] AS [ServiceType],
	CAST(a.[StNum] AS varchar) + ' ' + st.[FullStName] AS [ServiceAddress],
	tn.[TownName] AS [ServiceCity],
	a.[State] AS [ServiceState],
	a.[Zip] AS [ServiceZip],
	a.[PremNum] AS [PremiseNumber],
	oc.[MailCo] AS [OpCntrName],
	oc.[MailAdd] AS [OpCntrAddress],
	oc.[MailCSZ] AS [OpCntrCSZ],
	oc.[FaxNum] AS [OpCntrFax]
FROM
	[tblNJAWService] AS a
LEFT JOIN
	[Streets] AS st
ON
	a.[StName] = st.[StreetID]
LEFT JOIN
	[Towns] AS tn
ON
	a.[Town] = tn.[TownID]
LEFT JOIN
	[OperatingCenters] AS oc
ON
	a.[OpCntr] = oc.[OperatingCenterCode]
WHERE
	a.[RecID] = @RecID</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
