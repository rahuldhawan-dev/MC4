          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="emp_LicensesTableAdapter" GeneratorDataComponentClassName="emp_LicensesTableAdapter" Name="emp_Licenses" UserDataComponentName="emp_LicensesTableAdapter">
            <MainSource>
              <DbSource ConnectionRef="ProductionSqlServer (Web.config)" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT
	s.[Description] AS [Status],
	oc.OperatingCenterCode as [OpCode],
	eml.[Position_Category],
	d.[Description] as [Department_Name],
	e.[Last_Name],
	e.[First_Name],
	pc.[Position],
	pc.[License_Requirement_Attainment],
	e.[License_Water_Treatment],
	e.[T_License],
	e.[Date_of_T_License],
	e.[License_Water_Distribution],
	e.[W_License],
	e.[Date_of_W_License],
	e.[License_Sewer_Collection],
	e.[C_License],
	e.[Date_of_C_License],
	e.[License_Sewer_Treatment],
	e.[S_License],
	e.[Date_of_S_License],
	e.[License_Industrial_Discharge],
	e.[N_License],
	e.[Date_of_N_License],
	e.[CDL],
	e.[Drivers_License]
FROM
	[tblEmployee] AS e
LEFT JOIN
	[tblEmployee_Master_List] AS eml
ON
	e.[EmployeeID] = eml.[EmployeeID]
LEFT JOIN
	[tblPosition_History] AS ph
ON
	e.[tblEmployeeID] = ph.[tblEmployeeID]
LEFT JOIN
	tblPositions_Classifications AS pc
ON
	ph.[Position_ID] = pc.[PositionID]
LEFT JOIN
	EmployeeStatuses AS s
ON
	s.Id = e.StatusId
LEFT JOIN
	EmployeeDepartments AS d
ON
	e.DepartmentId = d.Id
LEFT JOIN
  OperatingCenters oc on oc.OperatingCenterId = e.OperatingCenterId
WHERE
	oc.OperatingCenterCode = @OpCode
ORDER BY
	s.[Description], eml.[Position_Category], d.[Description], e.[Last_Name], e.[First_Name]
</CommandText>
