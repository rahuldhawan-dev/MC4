          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="emp_SickBank_QATableAdapter" GeneratorDataComponentClassName="emp_SickBank_QATableAdapter" Name="emp_SickBank_QA" UserDataComponentName="emp_SickBank_QATableAdapter">
            <MainSource>
              <DbSource ConnectionRef="ProductionSqlServer (Web.config)" DbObjectName="MCProd.dbo.tblEmployee" DbObjectType="Table" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT
	IsNull(e.Last_Name + ', ', '') + IsNull(e.First_Name, '') AS [Name],
	e.EmployeeID
FROM
	tblEmployee AS e
INNER JOIN
	tblPosition_History AS ph
ON
	ph.tblEmployeeID = e.tblEmployeeID
AND
	ph.Position_History_ID = (SELECT MAX(Position_History_ID) FROM tblPosition_History WHERE tblEmployeeID = e.tblEmployeeID)
INNER JOIN
	tblPositions_Classifications AS pc
ON
	pc.PositionID = ph.Position_ID
INNER JOIN
	EmployeeStatuses AS s
ON
	s.Id = e.StatusId
LEFT JOIN
  OperatingCenters oc on oc.OperatingCenterId = e.OperatingCenterId
WHERE
	s.[Description] = 'Active'
AND
	oc.OperatingCenterCode = @OpCode
AND
	pc.Category = 'Bargaining Unit'
AND
	NOT EXISTS (SELECT 1 FROM tblEmployee_Sick_Bank WHERE EmployeeID = e.tblEmployeeID)
ORDER BY
	e.Last_Name, e.First_Name</CommandText>
                    <Parameters>
                      <Parameter AllowDbNull="true" AutogeneratedName="OpCode" ColumnName="OpCode" DataSourceName="MCProd.dbo.tblEmployee" DataTypeServer="nvarchar(255)" DbType="String" Direction="Input" ParameterName="@OpCode" Precision="0" ProviderType="NVarChar" Scale="0" Size="255" SourceColumn="OpCode" SourceColumnNullMapping="false" SourceVersion="Current" />
                    </Parameters>
                  </DbCommand>
