                      <Parameter AllowDbNull="true" AutogeneratedName="OpCode" ColumnName="OpCode" DataSourceName="MCProd.dbo.tblEmployee" DataTypeServer="nvarchar(255)" DbType="String" Direction="Input" ParameterName="@OpCode" Precision="0" ProviderType="NVarChar" Scale="0" Size="255" SourceColumn="OpCode" SourceColumnNullMapping="false" SourceVersion="Current" />
