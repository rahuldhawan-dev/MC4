          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="emp_SickBankTableAdapter" GeneratorDataComponentClassName="emp_SickBankTableAdapter" Name="emp_SickBank" UserDataComponentName="emp_SickBankTableAdapter">
            <MainSource>
              <DbSource ConnectionRef="ProductionSqlServer (Web.config)" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT
	oc.OperatingCenterCode as OpCode,
	e.Last_Name,
	e.First_Name,
	l1.LookupValue AS [Entry Type], 
	l2.LookupValue AS [Year],
	esb.Hours
FROM
	dbo.tblEmployee AS e
INNER JOIN
	dbo.tblEmployee_Sick_Bank AS esb
ON
	e.tblEmployeeID = esb.EmployeeID
INNER JOIN
	dbo.Lookup AS l1
ON
	esb.Entry_Type = l1.LookupID
INNER JOIN
	dbo.Lookup AS l2
ON
	esb.Calendar_Year = l2.LookupID
INNER JOIN
	dbo.tblPosition_History AS ph
ON
	ph.tblEmployeeID = e.tblEmployeeID
AND
	ph.Position_History_ID = (SELECT MAX(Position_History_ID) FROM tblPosition_History WHERE tblEmployeeID = e.tblEmployeeID)
INNER JOIN
	dbo.tblPositions_Classifications AS pc
ON
	pc.PositionID = ph.Position_ID
LEFT JOIN
	EmployeeStatuses AS s
ON
	s.Id = e.StatusId
LEFT JOIN
	OperatingCenters oc on oc.OperatingCenterId = e.OperatingCenterID
WHERE
	oc.OperatingCenterCode in (select value from dbo.ParmsToList(@OpCode))
AND
	(@status is null OR @status = s.[Description])
AND
	(@businessUnitID is null or pc.BusinessUnitID = @businessUnitID)
AND
	pc.Category = 'Bargaining Unit'
ORDER BY
	oc.OperatingCenterCode ASC,
	e.Last_Name ASC,
	e.First_Name ASC,
	l2.LookupValue ASC,
	l1.LookupValue ASC
</CommandText>
                    <Parameters>
                      <Parameter AllowDbNull="false" AutogeneratedName="status" ColumnName="" DataSourceName="" DataTypeServer="unknown" DbType="String" Direction="Input" ParameterName="@status" Precision="0" Scale="0" Size="1024" SourceColumn="" SourceColumnNullMapping="false" SourceVersion="Current" />
