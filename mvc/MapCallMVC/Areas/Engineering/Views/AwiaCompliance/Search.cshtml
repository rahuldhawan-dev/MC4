@model MapCallMVC.Areas.Engineering.Models.ViewModels.AwiaCompliance.SearchAwiaCompliance

@using (Form.BeginForm("Index", "AwiaCompliance", "Engineering"))
{
    <div class="field-column fc200 searchFields">
        @Html.EditorFor(x => x.Id)
        @Html.EditorFor(x => x.State)
        @Html.EditorFor(x => x.OperatingCenter)
        @Html.EditorFor(x => x.PublicWaterSupplies)
        @Html.EditorFor(x => x.CertificationType)
        @Html.EditorFor(x => x.CreatedBy)
        @Html.EditorFor(x => x.CertifiedBy)
        @Html.EditorFor(x => x.DateSubmitted)
        @Html.EditorFor(x => x.DateAccepted)
        @Html.EditorFor(x => x.RecertificationDue)

        @Html.ButtonGroup(@<text>
                              @Control.SubmitButton("Search")
                              @Control.ResetButton()
                           </text>)
    </div>
}

