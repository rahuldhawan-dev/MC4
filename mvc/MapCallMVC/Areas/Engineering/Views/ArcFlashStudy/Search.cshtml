@model MapCallMVC.Areas.Engineering.Models.ViewModels.ArcFlash.SearchArcFlashStudy

@using (Form.BeginForm("Index", "ArcFlashStudy"))
{
    <div class="field-column fc200 searchFields">
        @Html.EditorFor(x => x.State)
        @Html.EditorFor(x => x.OperatingCenter)
        @Html.EditorFor(x => x.Facility)
        @Html.EditorFor(x => x.ArcFlashStatus)
        @Html.EditorFor(x => x.DateLabelsApplied)
        @Html.ButtonGroup(@<text>
                              @Control.SubmitButton("Search").WithId("Search")
                              @Control.ResetButton()
                           </text>)
    </div>
}

