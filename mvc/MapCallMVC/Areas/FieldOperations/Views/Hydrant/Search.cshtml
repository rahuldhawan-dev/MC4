@model MapCallMVC.Areas.FieldOperations.Models.ViewModels.SearchHydrant

@Html.ScriptFor("~/Scripts/Areas/FieldOperations/Hydrant/Search.js", ScriptLoadType.LoadFromPartial)

@using (Form.BeginForm("Index", "Hydrant"))
{
    <div class="field-column fc200 searchFields">
        @Html.EditorFor(x => x.State, new { html = new { autofocus = "autofocus" } })
        @Html.EditorFor(x => x.OperatingCenter)
        @Html.EditorFor(x => x.WaterSystem)
        @Html.EditorFor(x => x.Town)
        @Html.EditorFor(x => x.Gradient)
        @Html.EditorFor(x => x.HydrantNumber)
        @Html.EditorFor(x => x.SAPEquipmentId)
        @Html.EditorFor(x => x.SAPErrorCode)
        @Html.EditorFor(x => x.HasSAPErrorCode)
        @Html.EditorFor(x => x.TownSection)
        @Html.EditorFor(x => x.FireDistrict)
        @Html.EditorFor(x => x.WorkOrderNumber)
        @Html.EditorFor(x => x.StreetNumber)
        @Html.EditorFor(x => x.Street)
        @Html.EditorFor(x => x.CrossStreet)
        @Html.EditorFor(x => x.HydrantSuffix)
        @Html.EditorFor(x => x.LegacyId)
        @Html.EditorFor(x => x.GISUID)
        @Html.EditorFor(x => x.FunctionalLocation)
        @Html.EditorFor(x => x.FunctionalLocationDescription)
        @Html.EditorFor(x => x.Route)
        @Html.EditorFor(x => x.Zone)
        @Html.EditorFor(x => x.PaintingZone)
        @Html.EditorFor(x => x.Status)
        @Html.EditorFor(x => x.DateInstalled)
        @Html.EditorFor(x => x.UpdatedAt)
        @Html.EditorFor(x => x.MapPage)
        @Html.EditorFor(x => x.HydrantManufacturer)
        @Html.EditorFor(x => x.YearManufactured)
        @Html.EditorFor(x => x.HydrantBilling)
        @Html.EditorFor(x => x.OutOfService)
        @Html.EditorFor(x => x.RequiresInspection)
        @Html.EditorFor(x => x.RequiresPainting)
        @Html.EditorFor(x => x.LastPaintedDate)
        @Html.EditorFor(x => x.CreatedAt)
        @Html.EditorFor(x => x.LastInspectionDate)
        @Html.EditorFor(x => x.Critical)
        @Html.EditorFor(x => x.HydrantType)
        @Html.EditorFor(x => x.HydrantOutletConfiguration)
        @Html.EditorFor(x => x.HasWorkOrder)
        @Html.EditorFor(x => x.HasOpenWorkOrder)
        @Html.EditorFor(x => x.OpenWorkOrderWorkDescription)
        @Html.EditorFor(x => x.DateRetired)

        @Html.ButtonGroup(@<text>
            @Control.SubmitButton("Search").WithId("Search")
            @Control.ResetButton()
        </text>)
    </div>
}