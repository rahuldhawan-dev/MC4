@using MapCallMVC.Areas.FieldOperations.Models.ViewModels.GeneralWorkOrder
@model WorkOrder

@{
    var additionalDetailsViewModel = ViewModelFactory.Build<EditWorkOrderAdditional, WorkOrder>(Model);
	var complianceDataViewModel = ViewModelFactory.Build<EditWorkOrderComplianceData, WorkOrder>(Model);
}

@Html.ScriptFor("~/Scripts/Areas/FieldOperations/WorkOrder/Additional.js", ScriptLoadType.LoadFromPartial)

<fieldset>
    @Html.Partial("~/Areas/FieldOperations/Views/WorkOrder/_EditWorkOrderAdditionalDetails.cshtml", additionalDetailsViewModel)

    @if (Model.AssetType.Id == AssetType.Indices.SERVICE)
    {
        @Html.Partial("~/Areas/FieldOperations/Views/WorkOrder/_EditComplianceData.cshtml", complianceDataViewModel)
    }
</fieldset>