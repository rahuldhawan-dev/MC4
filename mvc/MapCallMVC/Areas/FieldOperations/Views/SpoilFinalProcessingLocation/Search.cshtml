@model MapCallMVC.Areas.FieldOperations.Models.ViewModels.SearchSpoilFinalProcessingLocation

@using (var f = Form.BeginForm("Index", "SpoilFinalProcessingLocation"))
{
<div class="field-column fc200">
    @Html.EditorFor(x => x.State)
    @Html.EditorFor(x => x.OperatingCenter)
    @Html.EditorFor(x => x.Town)
    @Html.EditorFor(x => x.Street)
    @Html.EditorFor(x => x.Name)


    @Html.ButtonGroup(@<text>
        @Control.SubmitButton("Search").WithId("Search")
        @Control.ResetButton()
    </text>)
</div>
}
