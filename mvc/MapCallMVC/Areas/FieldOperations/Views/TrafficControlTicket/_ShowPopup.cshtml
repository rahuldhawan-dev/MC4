@model TrafficControlTicket
           
<div class="esri-info">
    <h2>
        @Html.DefaultActionLink(Model.Id.ToString(), "Show", "TrafficControlTicket", "FieldOperations", new { id = Model.Id }, new { target = "_blank" })
    </h2>
    
    <div class="field-column fc100">
        @Html.DisplayFor(x => x.Id)
        @Html.DisplayFor(x => x.OperatingCenter)
        @Html.DisplayFor(x => x.Town)
        @Html.DisplayFor(x => x.StreetNumber)
        @Html.DisplayFor(x => x.Street)
    </div>
</div>