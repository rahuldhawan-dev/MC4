@model MapCallMVC.Areas.FieldOperations.Models.ViewModels.SearchAsBuiltImage

@using (Form.BeginForm("Index", "AsBuiltImage"))
{
    <div class="field-column fc200 searchFields">
        @Html.EditorFor(x => x.TaskNumber)
        @Html.EditorFor(x => x.OperatingCenter)
        @Html.EditorFor(x => x.State)
        @Html.EditorFor(x => x.County)
        @Html.EditorFor(x => x.Town)
        @Html.EditorFor(x => x.TownSection)
        @Html.EditorFor(x => x.ProjectName)
        @Html.EditorFor(x => x.StreetPrefix)
        @Html.EditorFor(x => x.ApartmentNumber)
        @Html.EditorFor(x => x.Street)
        @Html.EditorFor(x => x.StreetSuffix)
        @Html.EditorFor(x => x.CrossStreet)
        @Html.EditorFor(x => x.MapPage)
        @Html.EditorFor(x => x.DateInstalled)
        @Html.EditorFor(x => x.PhysicalInService)
        @Html.EditorFor(x => x.HasCoordinate)
        @Html.EditorFor(x => x.OfficeReviewRequired)
        @Html.EditorFor(x => x.EntityId)
        @Html.EditorFor(x => x.CoordinatesModifiedOn)
        @Html.EditorFor(x => x.CreatedAt)
        @Html.EditorFor(x => x.FileName)
        @Html.ButtonGroup(@<text>
            @Control.SubmitButton("Search").WithId("Search")
            @Control.ResetButton()
        </text>)
    </div>
}