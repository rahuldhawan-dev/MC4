@model MapCallMVC.Areas.Production.Models.ViewModels.SearchAssetReliability

@using (Form.BeginForm("Index", "AssetReliability", "Production"))
{
    <div class="field-column fc200 searchFields">
        @Html.EditorFor(x => x.State)
        @Html.EditorFor(x => x.OperatingCenter)
        @Html.EditorFor(x => x.Facility)
        @Html.EditorFor(x => x.EquipmentGroup)
        @Html.EditorFor(x => x.Equipment)
        @Html.EditorFor(x => x.Employee)
        @Html.ButtonGroup(@<text>
            @Control.SubmitButton("Search").WithId("Search")
            @Control.ResetButton()
        </text>)
    </div>
}