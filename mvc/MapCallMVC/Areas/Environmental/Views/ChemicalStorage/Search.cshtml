@model MapCallMVC.Areas.Environmental.Models.ViewModels.SearchChemicalStorage

@using (Form.BeginForm("Index", "ChemicalStorage")) {
    <div class="field-column fc200 searchFields">
        @Html.EditorFor(x => x.WarehouseNumber)
        @Html.EditorFor(x => x.IsActive)
        @Html.EditorFor(x => x.Chemical)
        @Html.EditorFor(x => x.State)
        @Html.EditorFor(x => x.OperatingCenter)
        @Html.EditorFor(x => x.Facility)
        @Html.EditorFor(x => x.Crtk)
        @Html.EditorFor(x => x.ChemicalForm)

        @Html.ButtonGroup(@<text>
            @Control.SubmitButton("Search").WithId("Search")
            @Control.ResetButton()
        </text>)
    </div>
}