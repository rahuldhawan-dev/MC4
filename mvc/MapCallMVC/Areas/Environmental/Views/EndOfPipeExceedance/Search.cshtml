@model MapCallMVC.Areas.Environmental.Models.ViewModels.SearchEndOfPipeExceedance
    
@using (Form.BeginForm("Index", "EndOfPipeExceedance"))
{
    <div class="field-column fc200 search-fields">
        @Html.EditorFor(x => x.State)
        @Html.EditorFor(x => x.OperatingCenter)
        @Html.EditorFor(x => x.WasteWaterSystem)
        @Html.EditorFor(x => x.Facility)
        @Html.EditorFor(x => x.LimitationType)
        @Html.EditorFor(x => x.EventDate)
        @Html.EditorFor(x => x.EndOfPipeExceedanceType)
        @Html.EditorFor(x => x.EndOfPipeExceedanceRootCause)
        @Html.EditorFor(x => x.ConsentOrder)
        @Html.EditorFor(x => x.NewAcquisition)

        @Html.ButtonGroup(@<text>
            @Control.SubmitButton("Search").WithId("Search")
            @Control.ResetButton()
        </text>)
    </div>
}