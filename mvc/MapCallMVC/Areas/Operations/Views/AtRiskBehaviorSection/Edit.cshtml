@model MapCallMVC.Areas.Operations.Models.ViewModels.AtRiskBehaviorSectionViewModel

@using (Form.BeginForm("Update", "AtRiskBehaviorSection", "Operations", new{ id = Model.Id }))
{
    <div class="field-column fc200">
        @Html.EditorFor(x => x.Description)
        @Html.EditorFor(x => x.SectionNumber)
        @Html.ButtonGroup(@<text>
            @Control.SubmitButton("Save")
            @Html.LinkButton("Cancel", "Show", "AtRiskBehaviorSection", new{ id = Model.Id })
        </text>)
    </div>
}