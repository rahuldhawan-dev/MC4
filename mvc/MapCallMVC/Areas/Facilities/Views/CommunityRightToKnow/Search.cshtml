@model MapCallMVC.Areas.Facilities.Models.ViewModels.SearchCommunityRightToKnow

@using (Form.BeginForm("Index", "CommunityRightToKnow"))
{
    <div class="field-column fc200 searchFields">
        @Html.EditorFor(x => x.State)
        @Html.EditorFor(x => x.OperatingCenter)
        @Html.EditorFor(x => x.Facility)
        @Html.EditorFor(x => x.SubmissionDate)
        @Html.EditorFor(x => x.ExpirationDate)
        @Html.EditorFor(x => x.Expired)
        @Html.ButtonGroup(@<text>
            @Control.SubmitButton("Search").WithId("Search")
            @Control.ResetButton()
        </text>)
    </div>
}

