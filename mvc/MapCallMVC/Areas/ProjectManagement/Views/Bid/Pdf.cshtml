@using MMSINC.ClassExtensions.IEnumerableExtensions
@inherits MapCall.Common.Views.PdfViewBase<MapCallMVC.Areas.ProjectManagement.Models.ViewModels.BidForm>
@{
    Layout = "~/Views/Shared/PdfBaseLayout.cshtml";
    TopMargin = MarginWidth.Inch;
    BottomMargin = MarginWidth.Inch;
    LeftMargin = MarginWidth.HalfInch;
    RightMargin = MarginWidth.HalfInch;

    var substantiallyComplete = (Model.Throwaways.SubstantiallyCompleteDate.HasValue) ? Model.Throwaways.SubstantiallyCompleteDate.Value.ToShortDateString() : "__________";
    var completeAndReadyForBilling = (Model.Throwaways.CompleteAndReadyForBillingDate.HasValue) ? @Model.Throwaways.CompleteAndReadyForBillingDate.Value.ToShortDateString() : "__________";
}

@section head {
    <style>
        * {
            font-size: 16pt;
            line-height: 1.2em;
        }

        html {
            padding: 0;
            padding-left: 0.5in;
            padding-right: 0.5in;
        }
        
         div#wrap {
             float: left;
             width: 100%;
         }

        div.upper-right {
            float: right;
            width: 50%;
            border: 1px solid black;
            padding: 8px;
        }

        div.body {
            clear: both;
            width: 100%;
        }

        td.blank-line {
            border-bottom: 1px solid black;
        }

        .center {
            text-align: center;
        }

        .disclaimer {
            font-style: italic;
        }

        .bold {
            font-weight: bold;
        }

        table {
            page-break-inside: avoid !important;
        }

        tbody {
            page-break-inside: auto !important;
        }

        table.article > thead > tr > th {
            font-weight: bold;
            text-align: left;
        }

        h1, h2, h3, h4, h5, h6, p,
        table.article > thead > tr > th,
        table.article > tbody > tr > td {
            padding-top: 8px;
            padding-bottom: 8px;
        }

        table.article > tbody > tr > td:first-child,
        table.bidder > tbody > tr > td:first-child {
            width: 5%;
            vertical-align: top;
        }

        table.article > tbody > tr > td:last-child, 
        table.bidder > tbody > tr > td:last-child {
            width: 95%;
        }

        table.addendums {
            width: 50%;
            left: 20%;
        }

        table.addendums > thead > tr > th,
        table.bidder > thead > tr > th {
            font-weight: normal;
            text-decoration: underline;
        }

        table.addendums thead tr th:first-child,
        table.addendums thead tr th:last-child {
            width: 45%;
        }

        .missing {
            font-weight: bold;
            color: red;
        }

        h1, h2, h3, h4, h5, h6,
        table.article,
        table.bidder {
            width: 100%;
        }

        table#unit-summary td, table#unit-summary th {
            text-align: left;
            border: 1px solid black;
            font-size: smaller;
        }

        table#unit-summary > tfoot > tr > td,
        table#unit-summary > tfoot > tr > th {
            border: none;
        }

        td.subtotal {
            border: 3px solid black !important;
        }

        table#contractor-signature {
            table-layout: fixed;
            page-break-after: always;
        }

        table#contractor-signature th {
            font-weight: normal;
            text-align: left;
        }

        table.bidder > thead > tr > th {
            text-align: left;
        }

        div.upper-right table,
        table.bidder > tbody > tr > td > table {
            width: 100%;
            table-layout: fixed;
        }

        table.bidder > tbody > tr > td > table > tbody > tr > td {
            padding-top: 10px;
            vertical-align: bottom;
        }
    </style>
}

@section body {
    <div id="wrap">
        <div class="upper-right">
            <table>
                <caption>BID FORM</caption>
                <tbody>
                    <tr>
                        <td colspan="2">Date:</td>
                        <td class="blank-line" colspan="10">&nbsp;</td>
                        <td colspan="3" style="text-align:right">Time:</td>
                        <td class="blank-line" colspan="5">&nbsp;</td>
                    </tr>
                    <tr><td colspan="20">&nbsp;</td></tr>
                    <tr>
                        <td colspan="4">Signature:</td>
                        <td class="blank-line" colspan="16">&nbsp;</td>
                    </tr>
                    <tr><td colspan="20">&nbsp;</td></tr>
                    <tr>
                        <td colspan="3">Witness:</td>
                        <td class="blank-line" colspan="17">&nbsp;</td>
                    </tr>
                    <tr><td colspan="20">&nbsp;</td></tr>
                    <tr>
                        <td colspan="20">
