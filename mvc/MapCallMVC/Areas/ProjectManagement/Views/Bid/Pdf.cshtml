                <thead>
                    <tr><th colspan="2">New Jersey American Water</th></tr>
                    <tr><td colspan="2">&nbsp;</td></tr>
                    <tr><th colspan="2">Bid Form - Price Schedule</th></tr>
                    <tr><td colspan="2">&nbsp;</td></tr>
                </thead>
                <tbody>
                    <tr>
                        <td>Project Number: @Model.Project.ProjectNumber</td>
                        <td>Operating Center: @Model.Project.OperatingCenter.OperatingCenterName</td>
                    </tr>
                    <tr><td colspan="2">&nbsp;</td></tr>
                    <tr>
                        <td>Project Name: @Model.Project.ProjectName</td>
                        <td>Municipality: @Model.Project.Town.ShortName</td>
                    </tr>
                    <tr><td colspan="2">&nbsp;</td></tr>
                    <tr>
                        <td>Street Name: @Model.Project.Street</td>
                    </tr>
                </tbody>
            </table><br/><br/>

            <table id="unit-summary">
                <thead>
                    <tr>
                        <th colspan="6" id="caption">Unit Summary</th>
                    </tr>
                    <tr>
                        <th>ID:</th>
                        <th>Description:</th>
                        <th>Quantity:</th>
                        <th>Unit:</th>
                        <th>Unit Price:</th>
                        <th>Total:</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in @Model.Project.ContractorLaborCosts)
                    {
                        <tr>
                            <td>@item.ContractorLaborCost.StockNumber</td>
                            <td>@item.ContractorLaborCost.JobDescription</td>
                            <td>@item.Quantity</td>
                            <td>@item.ContractorLaborCost.Unit</td>
