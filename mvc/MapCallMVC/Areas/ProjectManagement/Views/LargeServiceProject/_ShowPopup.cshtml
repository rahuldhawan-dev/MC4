@model LargeServiceProject

<div class="esri-info">
    <h2>
        @Html.DefaultActionLink(Model.ProjectTitle, "Show", "LargeServiceProject", "ProjectManagement", new { id = Model.Id }, new { target = "_blank"})
    </h2>
    <div class="field-column fc100">
        @Html.DisplayFor(x => x.OperatingCenter)
        @Html.DisplayFor(x => x.Town)
        @Html.DisplayFor(x => x.ProjectAddress)
        @Html.DisplayFor(x => x.AssetCategory)
        @Html.DisplayFor(x => x.AssetType)
        @Html.DisplayFor(x => x.ProposedPipeDiameter)
    </div>
</div>