@model MapCallMVC.Areas.ProjectManagement.Models.ViewModels.SearchRoadwayImprovementNotification

@using (Form.BeginForm("Index", "RoadwayImprovementNotification")) {
    <div class="field-column fc200 searchFields">
        @Html.EditorFor(x => x.OperatingCenter)
        @Html.EditorFor(x => x.Town)
        @Html.EditorFor(x => x.ExpectedProjectStartDate)
        @Html.EditorFor(x => x.DateReceived)
        @Html.EditorFor(x => x.RoadwayImprovementNotificationStatus)
        @Html.EditorFor(x => x.Description)
        @Html.EditorFor(x => x.PreconMeetingDate)
        @Html.EditorFor(x => x.RoadwayImprovementNotificationPreconAction)
        @Html.ButtonGroup(@<text>
                               @Control.SubmitButton("Search").WithId("Search")
                               @Control.ResetButton()
                           </text>)
    </div>
}