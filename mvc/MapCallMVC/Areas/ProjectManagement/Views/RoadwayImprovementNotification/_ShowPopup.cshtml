@model RoadwayImprovementNotification
           
<h2>
    @Html.DefaultActionLink(string.Format("Roadway Improvement Notification: #{0}", Model.Id), "Show", "RoadwayImprovementNotification", "ProjectManagement", new { id = @Model.Id }, null)
</h2>

<div class="field-column fc100">
    @Html.DisplayFor(x => x.OperatingCenter)
    @Html.DisplayFor(x => x.Town)
    @Html.DisplayFor(x => x.ExpectedProjectStartDate)
    @Html.DisplayFor(x => x.Description)
</div>