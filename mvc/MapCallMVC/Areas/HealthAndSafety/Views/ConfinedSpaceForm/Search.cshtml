@model MapCallMVC.Areas.HealthAndSafety.Models.ViewModels.SearchConfinedSpaceForm
           
@using (Form.BeginForm("Index", "ConfinedSpaceForm"))
{
    <div class="field-column fc200 searchFields">
        @Html.EditorFor(x => x.State)
        @Html.EditorFor(x => x.OperatingCenter)
        @Html.EditorFor(x => x.Facility)
        @Html.EditorFor(x => x.EntrantType)
        @Html.EditorFor(x => x.GeneralDateTime)
        @Html.ButtonGroup(@<text>
            @Control.SubmitButton("Search").WithId("Search")
            @Control.ResetButton()
        </text>)
    </div>
}