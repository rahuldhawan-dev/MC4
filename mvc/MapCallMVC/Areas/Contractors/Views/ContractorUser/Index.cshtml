@model MapCallMVC.Areas.Contractors.Models.ViewModels.SearchContractorUser
	
@(Html.TableFor(x => x)
	.WithCaption("Records found: " + Model.Count)
	.TemplateColumnFor("Id", x => Html.ActionLink(x.Id.ToString(), "Show", new { x.Id }))
	.SortableColumnFor(x => x.Email)
	.SortableColumnFor(x => x.Contractor)
	.SortableColumnFor(x => x.IsAdmin)
	.SortableColumnFor(x => x.IsActive)
	.ColumnFor(x => x.HasAccess)
	.WithPaginatedFooter())