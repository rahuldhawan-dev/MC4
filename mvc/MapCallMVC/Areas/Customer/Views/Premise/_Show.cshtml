@model Premise

<div class="field-column fc200">
    @Html.DisplayFor(x => x.Id)
    @Html.DisplayFor(x => x.OperatingCenter)
    @Html.DisplayFor(x => x.PlanningPlant)
    @Html.DisplayFor(x => x.PremiseNumber)
    @Html.DisplayFor(x => x.ServiceAddressApartment)
    @Html.DisplayFor(x => x.ServiceAddressFraction)
    @Html.DisplayFor(x => x.ServiceAddressHouseNumber)
    @Html.DisplayFor(x => x.ServiceAddressStreet)
    @Html.DisplayFor(x => x.ServiceCity)
    @Html.DisplayFor(x => x.ServiceZip)
    @Html.DisplayFor(x => x.Coordinate)
    @Html.DisplayFor(x => x.ConnectionObject)
    @Html.RenderDisplayTemplate(Html.DisplayLabelFor("DeviceCategory"), @<text>@if (Model.DeviceCategory != null){@Model.DeviceCategory.TrimStart('0')}</text>)
    @Html.DisplayFor(x => x.DeviceLocation)
    @Html.DisplayFor(x => x.Installation)
    @Html.DisplayFor(x => x.PublicWaterSupply)
    @Html.DisplayFor(x => x.PremiseType)
</div>