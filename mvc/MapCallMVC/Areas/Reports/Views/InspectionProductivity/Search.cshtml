@model MapCallMVC.Areas.Reports.Models.SearchInspectionProductivity
           
@using (Form.BeginForm("Index", "InspectionProductivity"))
{
    <div class="field-column fc200 searchFields">
        @Html.EditorFor(x => x.StartDate)
        @Html.EditorFor(x => x.Week)
        @Html.EditorFor(x => x.OperatingCenter)
        @Html.EditorFor(x => x.InspectedBy)
        @Html.ButtonGroup(@<text>
            @Control.SubmitButton("Search").WithId("Search")
            @Control.ResetButton()
        </text>)
    </div>
}