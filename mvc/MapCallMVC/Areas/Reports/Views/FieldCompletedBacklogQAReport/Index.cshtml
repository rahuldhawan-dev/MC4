@model MapCallMVC.Areas.Reports.Models.SearchFieldCompletedBacklogQAReport

@{
    ActionBarHelper.AddExportLink();
    var ordered = Model.Results.OrderBy(x => x.OperatingCenter);
}

@(Html.TableFor(x => ordered)
