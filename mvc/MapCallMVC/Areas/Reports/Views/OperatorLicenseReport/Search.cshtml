@model MapCallMVC.Models.ViewModels.OperatorLicenses.SearchOperatorLicenseReport

@using (Form.BeginForm("Index", "OperatorLicenseReport"))
{
    <div class="field-column fc200 searchFields">
        @Html.EditorFor(x => x.State)
        @Html.EditorFor(x => x.OperatingCenter)
        @Html.EditorFor(x => x.Expired)
        @Html.EditorFor(x => x.LicensedOperatorOfRecord)
        @Html.EditorFor(x => x.EmployeeStatus)
        @Html.EditorFor(x => x.LicenseLevel)
        @Html.EditorFor(x => x.LicenseSubLevel)
        @Html.ButtonGroup(@<text>
                              @Control.SubmitButton("Search").WithId("Search")
                              @Control.ResetButton()
                           </text>)
    </div>
}