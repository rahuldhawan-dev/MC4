      .ColumnFor(x => x.OperatingCenter)
      .ColumnFor(x => x.Town)
      .ColumnFor(x => x.InspectionType)
      .ColumnFor(x => x.Jan)
      .ColumnFor(x => x.Feb)
      .ColumnFor(x => x.Mar)
      .ColumnFor(x => x.Apr)
      .ColumnFor(x => x.May)
      .ColumnFor(x => x.Jun)
      .ColumnFor(x => x.Jul)
      .ColumnFor(x => x.Aug)
      .ColumnFor(x => x.Sep)
      .ColumnFor(x => x.Oct)
      .ColumnFor(x => x.Nov)
      .ColumnFor(x => x.Dec)
      .ColumnFor(x => x.Total)
    )