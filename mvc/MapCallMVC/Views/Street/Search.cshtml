@model MapCallMVC.Models.ViewModels.Streets.SearchStreet

@using (Form.BeginForm("Index", "Street"))
{
    <div class="field-column fc200 searchFields">
        @Html.EditorFor(x => x.State)
        @Html.EditorFor(x => x.County)
        @Html.EditorFor(x => x.Town)
        @Html.EditorFor(x => x.FullStName)
        @Html.EditorFor(x => x.IsActive)
        @Html.ButtonGroup(@<text>
                              @Control.SubmitButton("Search").WithId("Search")
                              @Control.ResetButton()
                           </text>)
    </div>
}