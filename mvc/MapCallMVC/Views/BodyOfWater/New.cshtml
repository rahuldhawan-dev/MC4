@model MapCallMVC.Models.ViewModels.CreateBodyOfWater

@using (Form.BeginForm("Create", "BodyOfWater"))
{
    <div class="field-column fc200">
        @Html.EditorFor(x => x.State)
        @Html.EditorFor(x => x.OperatingCenter)
        @Html.EditorFor(x => x.Name)
        @Html.EditorFor(x => x.CriticalNotes)

        @Html.ButtonGroup(@<text>
                              @Control.SubmitButton("Save")
                              @Html.LinkButton("Cancel", "Search")
                           </text>)
    </div>
}