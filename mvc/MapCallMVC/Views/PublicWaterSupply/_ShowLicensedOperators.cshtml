@model PublicWaterSupply

        @(Html.TableFor(x => x.LicensedOperators, new { id = "OperatorLicenseTable"})
              .TemplateColumnFor("Employee", x => Html.ActionLink(x.LicensedOperator.Employee.ToString(), "Show", "OperatorLicense", new { id = x.LicensedOperator.Id }, new { area = " " }))
              .ColumnFor(x => x.LicensedOperator.OperatingCenter)
              .ColumnFor(x => x.LicensedOperator.OperatorLicenseType)
              .ColumnFor(x => x.LicensedOperator.LicenseLevel)
              .ColumnFor(x => x.LicensedOperator.LicenseSubLevel)
              .ColumnFor(x => x.LicensedOperator.LicenseNumber)
              .ColumnFor(x => x.LicensedOperator.ValidationDate)
              .ColumnFor(x => x.LicensedOperator.ExpirationDate))
