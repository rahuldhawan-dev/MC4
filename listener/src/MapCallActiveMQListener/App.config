<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <sectionGroup name="workOrders">
      <section name="activeMQ" type="MMSINC.Utilities.ActiveMQ.ActiveMQConfigurationSection, MMSINC.Core" />
    </sectionGroup>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
  </configSections>

  <appSettings>
    <add key="SAPWebServiceUserName" value="sys_mapcall" />
    <add key="SAPWebServicePassword" value="Amwater11" />
    <add key="SAPWebServiceUrl" value="https://gateway-dev.amwater.com/qa2/XISOAPAdapter/MessageServlet" />
    <add key="MakeChanges" value="true" />
    <add key="noreply_address" value="noreply@mapcall.amwater.com" />
  </appSettings>

  <connectionStrings>
    <add name="Main" connectionString="Data Source=localhost;Initial Catalog=MapCallDEV;Integrated Security=sspi;" />
  </connectionStrings>

  <workOrders>
    <activeMQ scheme="tcp" host="hsynlamqs001.amwaternp.net" port="61616" />
  </workOrders>
 
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
  </startup>

  <log4net>
    <appender name="RollingFileAppender" type="log4net.Appender.RollingFileAppender">
      <file value="log.txt" />
      <appendToFile value="true" />
      <rollingStyle value="Size" />
      <maxSizeRollBackups value="10" />
      <maximumFileSize value="100KB" />
      <staticLogFileName value="true" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %-5level %logger [%property{NDC}] - %message%newline" />
      </layout>
    </appender>

    <appender name="Work1ViewAppender" type="log4net.Appender.RollingFileAppender">
      <file value="work1view.txt" />
      <appendToFile value="true" />
      <rollingStyle value="Date" />
      <maxSizeRollBackups value="8" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%message%newline" />
      </layout>
      <filter type="log4net.Filter.StringMatchFilter">
        <stringToMatch value="-MapCallListener-" />
        <acceptOnMatch value="true" />
      </filter>
      <filter type="log4net.Filter.DenyAllFilter" />
    </appender>

    <root>
      <level value="INFO" />
      <appender-ref ref="RollingFileAppender" />
      <appender-ref ref="Work1ViewAppender" />
    </root>

    <logger name="NHibernate">
      <level value="ERROR" />
    </logger>

    <logger name="NHibernate.SQL">
      <level value="ERROR" />
    </logger>
  </log4net>

  <system.net>
    <mailSettings>
      <smtp deliveryMethod="Network" from="03npnoreply@amwater.com">
        <network host="mail.amwaternp.net" port="25" />
      </smtp>
    </mailSettings>
  </system.net>

